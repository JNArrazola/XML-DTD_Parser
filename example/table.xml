    <column>
      <name>username</name>
      <type>varchar</type>
      <length>255</length>
      <constraints>
        <constraint>unique</constraint>
        <constraint>not null</constraint>
      </constraints>
    </column>
    <column>
      <name>email</name>
      <type>varchar</type>
      <length>255</length>
      <constraints>
        <constraint>not null</constraint>
      </constraints>
    </column>
  </columns>
</table>
